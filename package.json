{
  "name": "genesis",
  "version": "0.0.1",
  "description": "App web para gestionar proyectos de tesis",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drakantas/Genesis.git"
  },
  "author": "Draku",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drakantas/Genesis/issues"
  },
  "homepage": "https://github.com/drakantas/Genesis#readme",
  "devDependencies": {
    "laravel-mix": "^1.4.2"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bootstrap-sass": "^3.3.7",
    "bootstrap-select": "^1.12.4",
    "codemirror": "^5.30.0",
    "cross-env": "^5.0.5",
    "eonasdan-bootstrap-datetimepicker": "^4.17.47",
    "jquery": "^3.2.1",
    "metismenu": "^2.7.0",
    "summernote": "^0.8.8"
  }
}
